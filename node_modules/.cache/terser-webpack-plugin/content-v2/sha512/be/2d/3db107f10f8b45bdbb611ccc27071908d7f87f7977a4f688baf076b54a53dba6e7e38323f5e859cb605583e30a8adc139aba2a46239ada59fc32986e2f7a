f;\\n  /*滚动条的基色*/\\n}\\n.pipe_c[data-v-568d0839] {\\n  overflow: hidden;\\n  position: absolute;\\n  width: 100%;\\n  height: 100%;\\n}\\n.pipe_row[data-v-568d0839] {\\n  background: -webkit-gradient(linear, left bottom, left top, from(#dde2e7), color-stop(#b6c0d0), to(#e3e8ec));\\n  background: linear-gradient(0deg, #dde2e7, #b6c0d0, #e3e8ec);\\n}\\n.pipe_com[data-v-568d0839] {\\n  background: -webkit-gradient(linear, right top, left top, from(#dde2e7), color-stop(#b6c0d0), to(#e3e8ec));\\n  background: linear-gradient(-90deg, #dde2e7, #b6c0d0, #e3e8ec);\\n}\\n.skew_row_right_bottom[data-v-568d0839] {\\n  -webkit-transform: skew(-45deg, 0deg);\\n          transform: skew(-45deg, 0deg);\\n  margin-left: -8px;\\n}\\n.skew_row_left_bottom[data-v-568d0839] {\\n  -webkit-transform: skew(45deg, 0deg);\\n          transform: skew(45deg, 0deg);\\n  margin-left: 8px;\\n}\\n.skew_row_left_top[data-v-568d0839] {\\n  -webkit-transform: skew(-45deg, 0deg);\\n          transform: skew(-45deg, 0deg);\\n  margin-left: 8px;\\n}\\n.skew_com_left_top[data-v-568d0839] {\\n  -webkit-transform: skew(0deg, -45deg);\\n          transform: skew(0deg, -45deg);\\n  margin-top: 8px;\\n}\\n.skew_com_right_top[data-v-568d0839] {\\n  -webkit-transform: skew(0deg, 45deg);\\n          transform: skew(0deg, 45deg);\\n  margin-top: 8px;\\n}\\n.skew_com_right_bottom[data-v-568d0839] {\\n  -webkit-transform: skew(0deg, -45deg);\\n          transform: skew(0deg, -45deg);\\n  margin-top: -8px;\\n}\\n.hump_row_right[data-v-568d0839] {\\n  border-radius: 0 9px 9px 0;\\n}\\n.hump_row_left[data-v-568d0839] {\\n  border-radius: 9px 0 0 9px;\\n}\\n.hump_com_bottom[data-v-568d0839] {\\n  border-radius: 0 0 9px 9px;\\n}\\n.flex_com_top[data-v-568d0839] {\\n  display: -webkit-box;\\n  display: -ms-flexbox;\\n  display: flex;\\n  display: -webkit-flex;\\n  -webkit-box-orient: horizontal;\\n  -webkit-box-direction: normal;\\n      -ms-flex-direction: row;\\n          flex-direction: row;\\n  -webkit-box-align: end